# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

target 'LetsChat' do
  use_frameworks!

#FireBase
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'Firebase/Storage'

#Facebook
pod 'FBSDKCoreKit'
pod 'FBSDKLoginKit'
pod 'FBSDKShareKit'

#Google
pod 'GoogleSignIn'

pod 'MessageKit'
pod 'JGProgressHUD'
pod 'RealmSwift'
pod 'SDWebImage'



end
